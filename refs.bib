	journal = {The EMBO Journal},
	keywords = {alternative splicing, epithelial--mesenchymal transition, epithelial splicing regulatory proteins},
	number = {19},
	pages = {3286-3300},
	title = {An ESRP-regulated splicing programme is abrogated during the epithelial--mesenchymal transition},
	url = {https://www.embopress.org/doi/abs/10.1038/emboj.2010.195},
	volume = {29},
	year = {2010},
	bdsk-url-1 = {https://www.embopress.org/doi/abs/10.1038/emboj.2010.195},
